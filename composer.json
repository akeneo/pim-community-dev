    "require": {
        "ext-openssl": "*",
        "akeneo/api-php-client": "^9.0",
        "akeneo/pim-community-dev": "dev-master",
        "akeneo/pim-onboarder": "^7.0.0",
        "dragonmantank/cron-expression": "^3.3",
        "google/cloud-firestore": "^1.22",
        "grpc/grpc": "^1.42.0",
        "hslavich/oneloginsaml-bundle": "2.0.*",
        "jfcherng/php-diff": "^6.10",
        "justinrainbow/json-schema": "^5.2.11",
        "league/flysystem-google-cloud-storage": "^2.3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "openspout/openspout": "^4.8.0",
        "symfony/password-hasher": "5.4.*",
        "tigitz/php-spellchecker": "0.4.0"
    },
    "require-dev": {
        "akeneo/php-coupling-detector": "^0.7.0",
        "behat/mink-selenium2-driver": "^1.5.0",
        "behat/transliterator": "@stable",
        "blackfire/php-sdk": "^1.25",
        "friends-of-behat/mink": "^1.9.0",
        "friends-of-behat/mink-browserkit-driver": "^1.5.0",
        "friends-of-behat/mink-extension": "^2.5.0",
        "friends-of-behat/symfony-extension": "^2.2.1",
        "friends-of-phpspec/phpspec-code-coverage": "^6.1.0",
        "friendsofphp/php-cs-fixer": "^3.0,<3.9.1",
        "instaclick/php-webdriver": "1.4.7",
        "jfcherng/php-diff": "^6.10",
        "league/flysystem-aws-s3-v3": "^2.0",
        "liuggio/fastest": "1.8.*",
        "mcustiel/phiremock": "2.1.1",
        "phpspec/phpspec": "^7.1.0",
        "phpspec/prophecy": "^1.14.0",
        "phpstan/phpstan": "^1.6.0",
        "phpstan/phpstan-symfony": "^1.1.0",
        "phpstan/phpstan-webmozart-assert": "^1.1.0",
        "phpunit/phpunit": "^9.5",
        "psr/simple-cache": "^2.0",
        "rector/rector": "^0.13.10",
        "sebastian/exporter": "^4.0.0",
        "sensiolabs/behat-page-object-extension": "^2.3.3",
        "symfony/debug-bundle": "^5.4.0",
        "symfony/web-profiler-bundle": "^5.4.0",
        "nunomaduro/phpinsights": "^2.0",
        "vimeo/psalm": "^4.10"
    },
    "repositories": {
        "onboarder-extension": {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-onboarder.git",
            "branch": "master",
            "no-api": true
        },
        "grth": {
            "type": "path",
            "url": "grth/"
        },
        "tria": {
            "type": "path",
            "url": "tria/"
        }
    },
    "suggest": {
        "akeneo/catalogs": "In order to install one of the Akeneo catalogs"
    },
    "scripts": {
        "before-commit-re": [
            "php vendor/bin/php-coupling-detector detect --config-file=src/Akeneo/ReferenceEntity/tests/back/.php_cd.php src/Akeneo/ReferenceEntity/back",
            "php vendor/bin/php-coupling-detector list-unused-requirements --config-file=src/Akeneo/ReferenceEntity/tests/back/.php_cd.php src/Akeneo/ReferenceEntity/back",
            "php vendor/bin/phpstan analyse --configuration src/Akeneo/ReferenceEntity/tests/back/phpstan.neon.dist --ansi",
            "php vendor/bin/behat --config src/Akeneo/ReferenceEntity/tests/back/behat.yml.dist --no-interaction --colors",
            "php vendor/bin/phpunit -c src/Akeneo/ReferenceEntity/tests/back  --colors='always'",
            "php vendor/bin/php-cs-fixer fix --config=.php_cs.php --ansi",
            "php vendor/bin/phpspec run -c src/Akeneo/ReferenceEntity/tests/back/phpspec.yml.dist --no-interaction --ansi"
        ],
        "before-commit-am": [
            "php vendor/bin/php-coupling-detector detect --config-file=src/Akeneo/AssetManager/tests/back/.php_cd.php src/Akeneo/AssetManager/back",
            "php vendor/bin/php-coupling-detector list-unused-requirements --config-file=src/Akeneo/AssetManager/tests/back/.php_cd.php src/Akeneo/AssetManager/back",
            "php vendor/bin/phpstan analyse --configuration src/Akeneo/AssetManager/tests/back/phpstan.neon.dist --ansi",
            "php vendor/bin/behat --config src/Akeneo/AssetManager/tests/back/behat.yml.dist --no-interaction --colors",
            "php vendor/bin/phpunit -c src/Akeneo/AssetManager/tests/back --colors='always'",
            "php vendor/bin/php-cs-fixer fix --config=.php_cs.php --ansi",
            "php vendor/bin/phpspec run -c src/Akeneo/AssetManager/tests/back/phpspec.yml.dist --no-interaction --ansi"
        ]
    },
    "scripts-descriptions": {
        "before-commit-re": "Run all the checks on the Reference Entity Bounded Context",
        "before-commit-am": "Run all the checks on the Asset Manager Bounded Context"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "symfony": {
            "require": "5.4.*"
        }
    }
}
