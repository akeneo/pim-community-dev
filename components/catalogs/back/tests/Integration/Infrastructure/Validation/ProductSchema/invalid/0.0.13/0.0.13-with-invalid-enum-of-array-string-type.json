{
    "$id": "https://example.com/product",
    "$schema": "https://api.akeneo.com/mapping/product/0.0.13/schema",
    "description": "The data should match one item from enum at #/properties/colors/items/type",
    "properties": {
        "uuid": {
            "title": "Product uuid",
            "type": "string"
        },
        "colors": {
            "type": "array",
            "items": {
                "type": "number",
                "enum": ["Red", "Green", "Blue"]
            }
        }
    }
}
