{
  "$id": "https://example.com/product",
  "$schema": "https://api.akeneo.com/mapping/product/0.0.10/schema",
  "description": "The data (string) must match the type: number at #/properties/size/enum/3",
  "properties": {
    "uuid": {
      "title": "Product uuid",
      "type": "string"
    },
    "size": {
      "type": "number",
      "enum": [1, 2, 3, "four"]
    }
  }
}
