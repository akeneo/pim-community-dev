            "Pim\\Upgrade\\Schema\\Tests\\": [
                "vendor/akeneo/pim-community-dev/upgrades/test_schema/",
                "upgrades/test_schema/"
            ],
            "Akeneo\\Platform\\CommunicationChannel\\Test\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Platform/Bundle/CommunicationChannelBundle/back/tests",
            "Pimee\\Upgrade\\Schema\\Tests\\": "upgrades/test_schema/",
            "Akeneo\\Test\\Pim\\Automation\\RuleEngine\\": "src/Akeneo/Pim/Automation/RuleEngine/tests/back",
            "Akeneo\\Onboarder\\Test\\": "vendor/akeneo/pim-onboarder/tests/",
            "spec\\Akeneo\\AssetManager\\": "src/Akeneo/AssetManager/tests/back/spec",
            "spec\\Akeneo\\ReferenceEntity\\": "src/Akeneo/ReferenceEntity/tests/back/spec",
            "Akeneo\\Platform\\TailoredExport\\Test\\": "components/tailored-export/back/tests",
            "Specification\\Akeneo\\Platform\\TailoredExport\\": "components/tailored-export/back/tests/Specification",
            "Akeneo\\Platform\\TailoredImport\\Test\\": "components/tailored-import/back/tests",
            "Specification\\Akeneo\\Platform\\TailoredImport\\": "components/tailored-import/back/tests/Specification",
            "Akeneo\\Test\\Pim\\TableAttribute\\": "grth/src/Akeneo/Pim/TableAttribute/tests/back",
            "Akeneo\\Platform\\Syndication\\Test\\": "components/syndication/back/tests",
            "Akeneo\\Test\\Pim\\Enrichment\\Product\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Pim/Enrichment/Product/back/Test",
            "Akeneo\\Test\\Pim\\Enrichment\\Category\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Pim/Enrichment/Category/back/Test",
            "Akeneo\\Test\\Pim\\Structure\\Family\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Pim/Structure/Family/back/Test",
            "AkeneoEnterprise\\Test\\Pim\\Enrichment\\Category\\": "src/Akeneo/Pim/Enrichment/Category/back/Test",
            "Specification\\AkeneoEnterprise\\Pim\\Enrichment\\Category\\": "src/Akeneo/Pim/Enrichment/Category/back/Test/Specification",
            "Akeneo\\Test\\Category\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Category/back/tests",
            "AkeneoEnterprise\\Test\\Category\\": "src/Akeneo/Category/back/tests",
            "Akeneo\\SupplierPortal\\Retailer\\Test\\": "components/supplier-portal-retailer/back/tests",
            "Akeneo\\SupplierPortal\\Supplier\\Test\\": "components/supplier-portal-supplier/back/tests",
            "Akeneo\\Test\\Channel\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Channel/back/tests",
            "AkeneoEnterprise\\Test\\Channel\\": "src/Akeneo/Channel/back/tests",
            "Akeneo\\Platform\\JobAutomation\\Test\\": "components/job-automation/back/tests",
