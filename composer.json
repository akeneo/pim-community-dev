{
    "name": "akeneo/pim-enterprise-dev",
    "description": "Akeneo PIM, Enterprise Edition Bundles",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
            "Pim\\Upgrade\\": "upgrades/",
            "Akeneo\\ReferenceEntity\\": "src/Akeneo/ReferenceEntity/back",
            "Akeneo\\AssetManager\\": "src/Akeneo/AssetManager/back",
            "Akeneo\\Connectivity\\Connection\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Connectivity/Connection/back",
            "Akeneo\\Platform\\CommunicationChannel\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Platform/Bundle/CommunicationChannelBundle/back",
            "Akeneo\\Pim\\Automation\\DataQualityInsights\\": ["src/Akeneo/Pim/Automation/DataQualityInsights/back", "vendor/akeneo/pim-community-dev/src/Akeneo/Pim/Automation/DataQualityInsights/back"],
            "AkeneoEnterprise\\Connectivity\\Connection\\": "src/Akeneo/Connectivity/Connection/back"
        },
        "exclude-from-classmap": [
            "vendor/akeneo/pim-community-dev/src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "Context": "vendor/akeneo/pim-community-dev/tests/legacy/features/",
            "": "tests/legacy/features/"
        },
        "psr-4": {
            "Pim\\Behat\\": "vendor/akeneo/pim-community-dev/tests/legacy/features/Behat",
            "PimEnterprise\\Behat\\": "tests/legacy/features/Behat/",
            "AkeneoTest\\": "vendor/akeneo/pim-community-dev/tests/back",
            "AkeneoTestEnterprise\\": "tests/back",
            "Akeneo\\Test\\IntegrationTestsBundle\\": "vendor/akeneo/pim-community-dev/tests/back/Integration/IntegrationTestsBundle/",
            "AkeneoEnterprise\\Test\\IntegrationTestsBundle\\": "tests/back/Integration/IntegrationTestsBundle/",
            "Akeneo\\Test\\": "vendor/akeneo/pim-community-dev/tests/back/",
            "AkeneoEnterprise\\Test\\Acceptance\\": "tests/back/Acceptance/",
            "Akeneo\\Test\\Pim\\Automation\\DataQualityInsights\\": ["src/Akeneo/Pim/Automation/DataQualityInsights/tests/back", "vendor/akeneo/pim-community-dev/src/Akeneo/Pim/Automation/DataQualityInsights/tests/back"],
            "Akeneo\\ReferenceEntity\\": "src/Akeneo/ReferenceEntity/tests/back",
            "Akeneo\\AssetManager\\": "src/Akeneo/AssetManager/tests/back",
            "Akeneo\\Connectivity\\Connection\\Tests\\Acceptance\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Connectivity/Connection/back/tests/Acceptance",
            "Akeneo\\Connectivity\\Connection\\Tests\\EndToEnd\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Connectivity/Connection/tests/",
            "Pim\\Upgrade\\Schema\\Tests\\": "vendor/akeneo/pim-community-dev/upgrades/test_schema/",
            "Akeneo\\Platform\\CommunicationChannel\\Test\\": "vendor/akeneo/pim-community-dev/src/Akeneo/Platform/Bundle/CommunicationChannelBundle/back/tests",
            "Pimee\\Upgrade\\Schema\\Tests\\": "upgrades/test_schema/",
            "Akeneo\\Test\\Pim\\Automation\\RuleEngine\\": "src/Akeneo/Pim/Automation/RuleEngine/tests/back",
            "Akeneo\\Onboarder\\Test\\": "vendor/akeneo/pim-onboarder/tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "akeneo/pim-community-dev": "dev-master",
        "akeneo/pim-onboarder": "^4.2.1",
        "hslavich/oneloginsaml-bundle": "1.5.1",
        "justinrainbow/json-schema": "^5.2",
        "phpspec/php-diff": "1.1.3",
        "phpseclib/phpseclib": "2.0.29",
        "superbalist/flysystem-google-storage": "7.2.2",
        "grpc/grpc": "1.30.0",
        "mekras/php-speller": "^2.1.2"
    },
    "require-dev": {
        "akeneo/php-coupling-detector": "0.3.*",
        "symfony/debug-bundle": "4.4.7",
        "symfony/web-profiler-bundle": "4.4.7",
        "symfony/web-server-bundle": "4.4.7",
        "behat/behat": "3.5.0",
        "behat/gherkin": "4.5.1",
        "behat/mink": "1.7.1",
        "behat/mink-browserkit-driver": "1.3.3",
        "behat/mink-extension": "2.3.1",
        "behat/mink-selenium2-driver": "^1.3.1",
        "behat/symfony2-extension": "2.1.5",
        "behat/transliterator": "1.3.0",
        "blackfire/php-sdk": "^1.18",
        "league/flysystem-memory": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "phpspec/phpspec": "^6.0",
        "phpspec/prophecy": "v1.10.2",
        "phpunit/phpunit": "^8.0.4",
        "sebastian/exporter": "~3.1.0",
        "sensiolabs/behat-page-object-extension": "2.1.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "liuggio/fastest": "1.6.*",
        "phpstan/phpstan": "^0.12.25",
        "phpstan/phpstan-symfony": "^0.12.6",
        "mcustiel/phiremock": "1.12.1",
        "phpstan/phpstan-webmozart-assert": "^0.12.7"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-onboarder.git",
            "branch": "master"
        }
    ],
    "suggest": {
        "akeneo/catalogs": "In order to install one of the Akeneo catalogs"
    },
    "scripts": {
        "before-commit-re": [
            "php vendor/bin/php-coupling-detector detect --config-file=src/Akeneo/ReferenceEntity/tests/back/.php_cd.php src/Akeneo/ReferenceEntity/back",
            "php vendor/bin/phpstan analyse --configuration src/Akeneo/ReferenceEntity/tests/back/phpstan.neon.dist --ansi",
            "php vendor/bin/behat --config src/Akeneo/ReferenceEntity/tests/back/behat.yml.dist --no-interaction --colors",
            "php vendor/bin/phpunit -c src/Akeneo/ReferenceEntity/tests/back  --colors='always'",
            "php vendor/bin/php-cs-fixer fix --config=.php_cs.php --ansi",
            "php vendor/bin/phpspec run -c src/Akeneo/ReferenceEntity/tests/back/phpspec.yml.dist --no-interaction --ansi"
        ],
        "before-commit-am": [
            "php vendor/bin/php-coupling-detector detect --config-file=src/Akeneo/AssetManager/tests/back/.php_cd.php src/Akeneo/AssetManager/back",
            "php vendor/bin/phpstan analyse --configuration src/Akeneo/AssetManager/tests/back/phpstan.neon.dist --ansi",
            "php vendor/bin/behat --config src/Akeneo/AssetManager/tests/back/behat.yml.dist --no-interaction --colors",
            "php vendor/bin/phpunit -c src/Akeneo/AssetManager/tests/back --colors='always'",
            "php vendor/bin/php-cs-fixer fix --config=.php_cs.php --ansi",
            "php vendor/bin/phpspec run -c src/Akeneo/AssetManager/tests/back/phpspec.yml.dist --no-interaction --ansi"
        ]
    },
    "scripts-description": {
        "before-commit-re": "Run all the checks on the Reference Entity Bounded Context",
        "before-commit-am": "Run all the checks on the Asset Manager Bounded Context"
    },
    "minimum-stability": "stable"
}
