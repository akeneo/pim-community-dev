{
    "name": "oro/navigation-bundle",
    "type": "symfony-bundle",
    "description": "BAP Navigation Bundle",
    "keywords": ["BAP", "UI", "User Interface", "Menu", "Navigation", "Pinbar", "Shortcuts", "History"],
    "homepage": "https://github.com/laboro/NavigationBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "oro/ui-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "knplabs/knp-menu": "v2.0.0-alpha1",
        "knplabs/knp-menu-bundle": "v2.0.0-alpha1",
        "friendsofsymfony/rest-bundle": "0.11.*",
        "nelmio/api-doc-bundle": "dev-master",
        "symfony/assetic-bundle": "2.1.*",
        "kriswallsmith/assetic": "1.1.*@dev",
        "oro/requirejs-bundle": "dev-master"
    },
    "require-dev": {
        "doctrine/data-fixtures": "@dev",
        "doctrine/doctrine-fixtures-bundle": "@dev"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\NavigationBundle": "" }
    },
    "target-dir": "Oro/Bundle/NavigationBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
