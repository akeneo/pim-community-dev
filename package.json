{
  "name": "pim-enterprise-dev",
  "version": "1.8.0",
  "description": "Akeneo PIM Enterprise Application",
  "homepage": "http://www.akeneo.com",
  "private": true,
  "scripts": {
    "install": "npm install vendor/akeneo/pim-community-dev --no-save",
    "webpack": "NODE_PATH=node_modules webpack --config vendor/akeneo/pim-community-dev/webpack.config.js --env=prod",
    "webpack-watch": "NODE_PATH=node_modules webpack --progress --config vendor/akeneo/pim-community-dev/webpack.config.js --watch",
    "lint": "eslint 'src/**/*.js' --config vendor/akeneo/pim-community-dev/.eslintrc --ignore-path vendor/akeneo/pim-community-dev/.eslintignore --quiet",
    "lint-fix": "eslint 'src/**/*.js' --config vendor/akeneo/pim-community-dev/.eslintrc --ignore-path vendor/akeneo/pim-community-dev/.eslintignore --quiet --fix"
  },
  "dependencies": {}
}
