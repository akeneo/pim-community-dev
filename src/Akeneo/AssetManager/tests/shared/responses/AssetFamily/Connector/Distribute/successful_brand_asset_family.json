{
  "request": {
    "method": "GET",
    "route": "akeneo_asset_manager_asset_family_rest_connector_get",
    "query": {
      "code": "brand"
    }
  },
  "response": {
    "status": 200,
    "body": {
      "code": "brand",
      "labels": {
        "fr_FR": "Marque"
      },
      "product_link_rules": [
        {
          "product_selections": [
            {
              "field": "sku",
              "operator": "EQUALS",
              "value": "{{product_ref}}",
              "locale": null
            }
          ],
          "assign_assets_to": [
            {
              "attribute": "user_instructions",
              "locale": "{{locale}}",
              "mode": "replace"
            }
          ]
        }
      ]
    }
  }
}
