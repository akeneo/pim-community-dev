includes:
    - vendor/phpstan/phpstan-symfony/extension.neon
parameters:
    reportUnmatchedIgnoredErrors: false
    excludes_analyse:
        - %currentWorkingDirectory%/src/Akeneo/Pim/Automation/FranklinInsights/tests
        - %currentWorkingDirectory%/src/Akeneo/Pim/Automation/DataQualityInsights/tests
        - %currentWorkingDirectory%/vendor/akeneo/pim-community-dev/src/Akeneo/Pim/Automation/DataQualityInsights/tests
    symfony:
        container_xml_path: %rootDir%/../../../var/cache/dev/srcKernelDevDebugContainer.xml
    ignoreErrors:
        - '#Unsafe usage of new static\(\)\.#'
