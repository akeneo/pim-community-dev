{
    "name": "oro/calendar-bundle",
    "type": "symfony-bundle",
    "description": "BAP Calendar Bundle",
    "keywords": ["BAP", "Calendar"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "oro/ui-bundle": "dev-master",
        "oro/windows-bundle": "dev-master",
        "oro/form-bundle": "dev-master",
        "oro/email-bundle": "dev-master",
        "oro/notification-bundle": "dev-master",
        "oro/locale-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "oro/cron-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\CalendarBundle": "" }
    },
    "target-dir": "Oro/Bundle/CalendarBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
