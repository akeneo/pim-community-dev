{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="icon"><img style="width:28px" src="data:image/png;base64," alt=""/></span>
        <span class="sf-toolbar-status">{{ collector.version.patch }}</span>
        {% if collector.isWarning %}<span class="sf-toolbar-status sf-toolbar-status-yellow">!</span>{% endif %}
    {% endset %}

    {% set text %}
        {% if collector.hasMongoDb == false %}
            <div class="sf-toolbar-info-piece">
                <b>Might requires MongoDB</b>
                <span class="sf-toolbar-status sf-toolbar-status-{{ collector.requireMongoDb ? 'yellow' : 'green' }}">{{ collector.requireMongoDb ? 'Yes' : 'No' }} </span>
            </div>
        {% else %}
            <div class="sf-toolbar-info-piece">
                <b>MongoDB enabled</b>
                <span class="sf-toolbar-status sf-toolbar-status-{{ collector.hasMongoDb ? 'green' : 'red' }}">{{ collector.hasMongoDb ? 'Yes' : 'No' }}</span>
            </div>
        {% endif %}
    {% endset %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}
{% endblock %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img style="width:28px" src="data:image/png;base64," alt=""/></span>
        <strong>Akeneo PIM</strong>
    </span>
{% endblock %}

{% block panel %}
    <h2>Akeneo PIM</h2>

    <h3>MongoDB support</h3>
        {% if collector.hasMongoDb %}
            <p>MongoDB is enabled on your installation.</p>
        {% else %}
            {% if collector.requireMongoDb %}
                <p>Your current installation has more than 5M product values but MongoDB isn't installed yet. <br />
                You should consider enabling MongoDB.</p>
            {% elseif not collector.requireMongoDb %}
                <p>Your current installation has less than 5M product values and MongoDB isn't installed yet.<br />
                Please note that if you  were to have more than 5M of product values, you will have to consider enabling MongoDB.</p>
            {% endif %}
            <p>Please read <a href="http://docs.akeneo.com/{{ collector.version.minor }}/reference/performances_guide/index.html#more-than-5-millions-of-product-values">this documentation</a> for more details.<br />
            You can also follow <a href="http://docs.akeneo.com/{{ collector.version.minor }}/developer_guide/installation/installation_workstation.html#installing-mongodb-and-enabling-it-is-as-catalog-storage">this guide</a> to know how to enable MongoDB support.</p>
        {% endif %}
{% endblock %}
