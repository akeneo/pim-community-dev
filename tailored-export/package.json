{
  "name": "akeneo-tailored-export",
  "version": "0.1.0",
  "private": true,
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "babel-loader": "8.1.0",
    "eslint": "^7.11.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "@types/styled-components": "^5.1.2",
    "akeneo-design-system": "link:../akeneo-design-system"
  },
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lib:build": "tsc -p ./lib.tsconfig.json",
    "translation:build": "cp ../public/js/translation/en_US.js ./src/translations.json",
    "route:build": "cp ../public/js/routes.js ./src/routes.json && sed -i 's/fos.Router.setData(//g' ./src/routes.json && sed -i 's/);//g' ./src/routes.json",
    "dependencies:build": "yarn --cwd=../legacy run lib:build && yarn --cwd=../shared run lib:build && yarn --cwd=../akeneo-design-system run lib:build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "main": "lib/index.js",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "workspaces": [
    "../legacy",
    "../shared",
    "../akeneo-design-system"
  ]
}
