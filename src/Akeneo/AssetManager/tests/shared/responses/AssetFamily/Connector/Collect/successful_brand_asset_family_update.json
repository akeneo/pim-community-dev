          ],
          "assign_assets_to": [
            {
              "mode": "add",
              "attribute": "product_asset_collection"
            }
          ]
        }
      ]
    }
  },
  "response": {
    "status": 204,
    "body": "",
    "headers": {
      "location": "http://localhost/api/rest/v1/asset-families/brand"
    }
  }
}

