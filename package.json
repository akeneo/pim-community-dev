{
  "name": "pim-enterprise-dev",
  "version": "3.2",
  "description": "Akeneo PIM Enterprise Application",
  "homepage": "http://www.akeneo.com",
  "private": true,
  "config": {
    "source": "vendor/akeneo/pim-community-dev",
    "sync": "vendor/akeneo/pim-community-dev/frontend/build/check-dependencies.js",
    "styles": "vendor/akeneo/pim-community-dev/frontend/build/compile-less.js"
  },
  "scripts": {
    "requirements": "node $npm_package_config_source/frontend/build/check-requirements.js",
    "sync": "NODE_PATH=node_modules node $npm_package_config_sync --source=$npm_package_config_source",
    "webpack": "yarn requirements && NODE_PATH=node_modules webpack --config $npm_package_config_source/webpack.config.js --env=prod",
    "webpack-dev": "yarn requirements && NODE_PATH=node_modules webpack --config $npm_package_config_source/webpack.config.js",
    "webpack-watch": "yarn requirements && NODE_PATH=node_modules webpack --progress --config $npm_package_config_source/webpack.config.js --watch",
    "prettier": "prettier --config $npm_package_config_source/.prettierrc.json --parser typescript --write \"./{src,tests}/Akeneo/ReferenceEntity/**/*.ts\"; prettier --config $npm_package_config_source/.prettierrc.json --parser typescript --write \"./{src,tests}/Akeneo/AssetManager/**/*.ts\"; prettier --config $npm_package_config_source/.prettierrc.json --parser typescript --write \"./{src,tests}/Akeneo/ReferenceEntity/**/*.tsx\"; prettier --config $npm_package_config_source/.prettierrc.json --parser typescript --write \"./{src,tests}/Akeneo/AssetManager/**/*.tsx\"; prettier --config $npm_package_config_source/.prettierrc.json --parser typescript --write \"./tests/front/**/*.js\";",
    "before-commit": "yarn prettier && yarn lint && yarn test",
    "test": "yarn unit && yarn webpack-test --env=prod && yarn integration && yarn acceptance-re && yarn acceptance-am && yarn acceptance tests/features && yarn acceptance $npm_package_config_source/tests/features",
    "webpack-test": "webpack --config $npm_package_config_source/webpack-test.config.js",
    "lint": "eslint 'src/**/*.js' --config $npm_package_config_source/.eslintrc --ignore-path $npm_package_config_source/.eslintignore --quiet",
    "lint-fix": "eslint 'src/**/*.js' --config $npm_package_config_source/.eslintrc --ignore-path $npm_package_config_source/.eslintignore --quiet --fix",
    "tslint": "node ./node_modules/tslint/bin/tslint",
    "unit": "jest --maxWorkers=4 --coverage --config tests/front/unit/jest/unit.jest.js",
    "integration": "jest --maxWorkers=4 --config $npm_package_config_source/tests/front/integration/jest/integration.jest.js",
    "acceptance": "NODE_PATH=./node_modules cucumber-js --parallel ${NODE_NUMBER_OF_THREAD:-2} --tags @acceptance-front -r ./frontend/test/acceptance/run-steps.js",
    "acceptance-am": "RANDOM_LATENCY=false NODE_PATH=./node_modules cucumber-js --tags @acceptance-front -r ./src/Akeneo/Pim/Enrichment/AssetManager/tests/front/acceptance/run-steps.js src/Akeneo/Pim/Enrichment/AssetManager/tests/front/features",
    "acceptance-re": "NODE_PATH=./node_modules cucumber-js --parallel ${NODE_NUMBER_OF_THREAD:-2} --tags @acceptance-front -r ./src/Akeneo/ReferenceEntity/tests/front/acceptance/run-steps.js src/Akeneo/ReferenceEntity/tests/features",
    "acceptance-report": "NODE_PATH=./node_modules yarn run acceptance -- -f json:web/test_dist/acceptance-js.json && node $npm_package_config_source/frontend/test/acceptance/generate-html-report.js",
    "less": "node $npm_package_config_styles",
    "less-watch": "SHELL=/bin/bash chokidar --verbose \"{$npm_package_config_source/src,src,web/bundles}/**/*.{less,css}\" -c \"yarn run less\"",
    "update-extensions": "node vendor/akeneo/pim-community-dev/frontend/build/update-extensions.js"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "@testing-library/react-hooks": "^2.0.3",
    "@testing-library/user-event": "^7.1.2",
    "@types/draft-js": "^0.10.25",
    "@types/enzyme": "^3.1.12",
    "@types/jest": "^24.0.22",
    "@types/styled-components": "^4.1.18",
    "babel-plugin-styled-components": "^1.10.6",
    "cucumber": "^4.0.0",
    "cucumber-html-reporter": "5.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "glob": "^7.1.2",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.3.1",
    "prettier": "^1.16.4",
    "puppeteer": "^2.0.0",
    "read-pkg": "4.0.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-react": "^4.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/types": "~7.11.5",
    "@types/backbone": "^1.3.42",
    "@types/node": "11.9.5",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.1.0",
    "@types/underscore": "~1.9.4",
    "JSON2": "0.1.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-minify": "0.5.0",
    "babel-polyfill": "6.26.0",
    "backbone": "0.9.10",
    "bundle-loader": "0.5.6",
    "chokidar-cli": "^1.2.2",
    "colors": "1.3.3",
    "deepmerge": "3.2.0",
    "dep-diff": "1.0.1",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "dropzone": "4.0.1",
    "eslint": "6.0.1",
    "exports-loader": "0.7.0",
    "expose-loader": "0.7.5",
    "extra-watch-webpack-plugin": "^1.0.3",
    "html-to-draftjs": "^1.4.0",
    "imports-loader": "0.8.0",
    "jquery": "3.4.0",
    "less": "^3.9.0",
    "loader-utils": "1.2.3",
    "lodash": "4.17.14",
    "merge-objects": "1.0.5",
    "raw-loader": "1.0.0",
    "react": "^16.8.3",
    "react-dom": "^16.9.0",
    "react-draft-wysiwyg": "^1.12.14",
    "react-redux": "^7.1.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^4.3.2",
    "summernote": "0.6.16",
    "text-loader": "0.0.1",
    "thread-loader": "^2.1.2",
    "ts-loader": "5.3.3",
    "typescript": "3.3.3333",
    "underscore": "1.8.3",
    "webpack": "4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-shell-plugin": "^0.5.0",
    "write-file-webpack-plugin": "4.5.0",
    "yamljs": "0.3.0",
    "react-markdown": "^4.3.1"
  },
  "resolutions": {
    "@babel/types": "~7.11.5",
    "@types/babel__traverse": "7.0.13",
    "jest/**/ip-regex": "2.1.0"
  }
}
