{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {# This toolbar item may appear along the top or bottom of the screen.#}
    {% set icon %}
        <span class="icon"><img style="width:30px" src="data:image/png;base64,
" alt=""/></span>
        <span class="sf-toolbar-status">{{ collector.version }}</span>
        {% if collector.isWarning %}<span class="sf-toolbar-status sf-toolbar-status-red">!</span>{%  endif %}
    {% endset %}

    {% set text %}

        {% if collector.hasMongoDb == false %}
            <div class="sf-toolbar-info-piece">
                <b>Requires MongoDB</b>
                <span class="sf-toolbar-status sf-toolbar-status-{{ collector.requireMongoDb ? 'red' : 'green' }}">{{ collector.requireMongoDb ? 'Yes' : 'No' }}</span>
            </div>
        {% else %}
            <div class="sf-toolbar-info-piece">
                <b>MongoDB enabled</b>
                <span class="sf-toolbar-status sf-toolbar-status-{{ collector.hasMongoDb ? 'green' : 'red' }}">{{ collector.hasMongoDb ? 'Yes' : 'No' }}</span>
            </div>
        {% endif %}
    {% endset %}

    {# Set the "link" value to false if you do not have a big "panel"
       section that you want to direct the user to. #}
    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { 'link': true }) }}

{% endblock %}

{% block head %}
    {# Optional, if you need your own JS or CSS files. #}
    {{ parent() }} {# Use parent() to keep the default styles #}
{% endblock %}

{% block menu %}
    {# This left-hand menu appears when using the full-screen profiler. #}
    <span class="label">
        <span class="icon"><img
                    src="data:image/png;base64,"
                    alt=""/></span>
        <strong>Example Collector</strong>
    </span>
{% endblock %}

{% block panel %}
    {# Optional, for showing the most details. #}
    <h2>Example</h2>
    <p>
        <em>Major information goes here</em>
    </p>
{% endblock %}
