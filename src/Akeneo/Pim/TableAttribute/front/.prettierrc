{
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true,
    "parser": "typescript",
    "printWidth": 80,
    "proseWrap": "never",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
}
