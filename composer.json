{
    "name": "akeneo/pim-community-dev",
    "description": "Akeneo PIM, the future of catalog management is open!",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/",
            "Context": "features/",
            "Pim\\Behat": "features/"
        },
        "psr-4": {
            "Pim\\Upgrade\\": "upgrades/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=5.4.4",
        "akeneo/measure-bundle": "0.5.0",
        "apy/jsfv-bundle": "2.0.1",
        "ass/xmlsecurity": "1.1.1",
        "doctrine/annotations": "1.2.7",
        "doctrine/cache": "1.5.4",
        "doctrine/common": "2.5.3",
        "doctrine/data-fixtures": "1.1.1",
        "doctrine/doctrine-bundle": "1.4.0",
        "doctrine/doctrine-fixtures-bundle": "2.3.0",
        "doctrine/doctrine-migrations-bundle": "1.1.1",
        "doctrine/migrations": "1.2.2",
        "doctrine/orm": "2.4.8",
        "dompdf/dompdf" : "0.6.2",
        "escapestudios/wsse-authentication-bundle": "2.2.1",
        "friendsofsymfony/jsrouting-bundle": "1.6.0",
        "friendsofsymfony/rest-bundle": "1.7.7",
        "gedmo/doctrine-extensions":"v2.4.13",
        "incenteev/composer-parameter-handler": "2.1.2",
        "jms/serializer-bundle": "1.1.0",
        "knplabs/knp-menu-bundle": "2.1.1",
        "kriswallsmith/assetic": "1.3.2",
        "leafo/lessphp": "0.5.0",
        "league/flysystem": "1.0.22",
        "league/flysystem-sftp": "1.0.9",
        "league/flysystem-ziparchive": "1.0.2",
        "liip/imagine-bundle": "1.5.3",
        "imagine/imagine": "0.6.3",
        "monolog/monolog": "1.19.0",
        "oneup/flysystem-bundle": "1.3.0",
        "sensio/distribution-bundle": "5.0.6",
        "sensio/framework-extra-bundle": "3.0.16",
        "sensio/generator-bundle": "3.0.6",
        "symfony/assetic-bundle": "2.8.0",
        "symfony/icu": "1.2.2",
        "symfony/monolog-bundle": "2.11.1",
        "symfony/swiftmailer-bundle": "2.3.11",
        "symfony/symfony": "2.8.6",
        "twig/extensions": "1.3.0",
        "box/spout": "2.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "squizlabs/php_codesniffer": "2.*",
        "pdepend/pdepend": "2.2.*",
        "phpmd/phpmd": "2.*",
        "behat/behat": "2.5.5",
        "kriswallsmith/buzz": ">=0.5",
        "behat/common-contexts": "1.2.0",
        "behat/gherkin":"2.3.5",
        "behat/mink":"1.7.1",
        "behat/mink-browserkit-driver": "1.3.2",
        "behat/mink-extension": "1.3.3",
        "behat/mink-selenium2-driver": "1.3.1",
        "behat/symfony2-extension": "1.1.2",
        "behat/transliterator":"1.0.1",
        "doctrine/mongodb-odm-bundle": "3.0.2",
        "sensiolabs/behat-page-object-extension": "1.0.1",
        "phpspec/phpspec": "2.5.*",
        "akeneo/phpspec-skip-example-extension": "1.2.*",
        "akeneo/php-coupling-detector": "dev-master",
        "symfony/phpunit-bridge": "~2.7"
    },
    "suggest": {
        "doctrine/mongodb-odm-bundle": "In order to activate the MongoDB support within Akeneo",
        "akeneo/catalogs": "In order to install one of the Akeneo catalogs"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "php app/console fos:js-routing:dump --target=web/js/routes.js"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "php app/console fos:js-routing:dump --target=web/js/routes.js"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "keep-outdated": true,
            "file": "app/config/parameters.yml",
            "env-map": {
                "database_host": "PIM_DATABASE_HOST",
                "database_port": "PIM_DATABASE_PORT",
                "database_name": "PIM_DATABASE_NAME",
                "database_user": "PIM_DATABASE_USER",
                "database_password": "PIM_DATABASE_PASSWORD"
            }
        },
        "branch-alias": {
            "dev-master": "1.6.x-dev",
            "dev-1.5": "1.5.x-dev",
            "dev-1.4": "1.4.x-dev",
            "dev-1.3": "1.3.x-dev",
            "dev-1.2": "1.2.x-dev",
            "dev-1.1": "1.1.x-dev",
            "dev-1.0": "1.0.x-dev"
        }
    }
}
