{% extends 'PimEnrichBundle:MassEditAction:configure.html.twig' %}

{%
    set stepLabels = [
        'pimee_product_asset.mass_edit_action.asset.steps.first',
        'pimee_product_asset.mass_edit_action.asset.steps.second',
        'pimee_product_asset.mass_edit_action.asset.steps.third'
    ]
%}

{% set title = 'pimee_product_asset.mass_edit_action.asset.page_title' %}
{% set subtitle = 'pimee_product_asset.mass_edit_action.asset.page_subtitle' %}
{% set confirmLabel = 'pimee_product_asset.mass_edit_action.asset.confirm' %}

{% set configureRoute = 'pimee_product_asset_mass_edit_asset_action_configure' %}
{% set performRoute = 'pimee_product_asset_mass_edit_asset_action_perform' %}

{% set nextStepNumber = 3 %}
{% set previousStepNumber = 2 %}

{% block head_script_component %}
    {{ parent() }}

    <script type="text/javascript">
        require(
            ['jquery', 'jquery.wizard'],
            function ($) {
                $('.wizard').wizard({ currentStep: 2});
            }
        );
    </script>
{% endblock %}
