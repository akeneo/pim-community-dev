{% extends 'PimConfigBundle::layout.html.twig' %}

{% set title = 'Currencies overview'|trans %}

{% block head_script %}
    {{ parent() }}

    {% include 'OroGridBundle:Include:javascript.html.twig' with {'datagridView': datagrid, 'selector': '#currency-grid'} %}
    {% include 'OroGridBundle:Include:stylesheet.html.twig' %}
{% endblock %}

{% block page_container %}
<div class="container-fluid">
    {% block content %}
    <div class="usser-row">
        <h1>{{ title }}</h1> 
        <div id="currency-grid"></div>
    </div>
    {% endblock %}
</div>
{% endblock %}
