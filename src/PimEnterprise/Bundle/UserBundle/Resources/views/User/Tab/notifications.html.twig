<div class="tab-pane {{ viewElement.loop.first ? 'active' : '' }}" id="{{ viewElement.alias|replace({' ': '-', '.': '-'})|lower }}">
    <div class="AknFormContainer AknFormContainer--withPadding">
        {% if form.proposalsToReviewNotification is defined %}
            {{ form_row(form.proposalsToReviewNotification) }}
        {% endif %}

        {% if form.proposalsStateNotification is defined %}
            {{ form_row(form.proposalsStateNotification) }}
        {% endif %}
    </div>
</div>
