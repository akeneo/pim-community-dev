{
  "request": {
    "method": "PATCH",
    "route": "akeneo_asset_manager_asset_family_rest_connector_create_or_update",
    "query": {
      "assetFamilyIdentifier": "brand"
    },
    "body": {
      "code": "brand",
      "labels": {
        "en_US": "Brand english label",
        "fr_FR": "Brand french label"
      },
      "rule_templates": [
        {
          "conditions": [
            {
              "field": "sku",
              "operator": "equals",
              "value": "{{product_sku}}"
            }
          ],
          "actions": [
            {
              "type": "add",
              "field": "{{attribute}}",
              "value": "{{code}}"
            }
          ]
        }
      ]
    }
  },
  "response": {
    "status": 204,
    "body": "",
    "headers": {
      "location": "http://localhost/api/rest/v1/asset-families/brand"
    }
  }
}

