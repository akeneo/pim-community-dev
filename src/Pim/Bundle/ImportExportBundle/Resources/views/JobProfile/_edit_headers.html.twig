{% set title =  (entityName ~ '.edit')|trans ~ ' - ' ~ jobInstance.label ~ ' [' ~ jobInstance.code ~ ']' %}

{% set left %}
    <ul class="inline">
        <li>{{ 'pim_import_export.job'|trans }}: {{ job.name|trans }}</li>
        <li>{{ 'pim_import_export.connector'|trans }}: {{ jobInstance.connector|trans }}</li>
    </ul>
{% endset %}

{% set titleButtons %}
    {{ elements.backLink(indexRoute) }}
{% endset %}

{% set buttons %}
    {{ elements.deleteLink(removeRoute, removeAcl, indexRoute, removeMessage, removeSuccessMessage) }}
    {{ elements.submitBtn('', 'ok') }}
{% endset %}

{{ elements.page_header(title, buttons, null, left, elements.updated(form.vars.id), titleButtons) }}

{{ elements.form_navbar(view_element_aliases(viewElementsAliases)) }}
