{
  "$id": "https://example.com/product",
  "$schema": "https://api.akeneo.com/mapping/product/0.0.13/schema",
  "description": "The data (boolean) must match the type: string at #/properties/colors/items/enum/2",
  "properties": {
    "uuid": {
      "title": "Product uuid",
      "type": "string"
    },
    "colors": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["Red", "Green", true]
      }
    }
  }
}
