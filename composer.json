    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
            "Pim\\Upgrade\\": "upgrades/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Context": "tests/legacy/features/"
        },
        "psr-4": {
            "Pim\\Behat\\": "tests/legacy/features/Behat",
