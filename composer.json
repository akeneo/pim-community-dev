{
    "name": "akeneo/pim-enterprise-dev",
    "description": "Akeneo PIM, Enterprise Edition Bundles",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/",
            "Context": "features/",
            "PimEnterprise\\Behat": "features/",
            "Pim\\Behat": "vendor/akeneo/pim-community-dev/features/"
        },
        "psr-4": {
            "Akeneo\\Test\\IntegrationTestsBundle\\": "vendor/akeneo/pim-community-dev/tests/IntegrationTestsBundle/",
            "AkeneoEnterprise\\Test\\IntegrationTestsBundle\\": "tests/IntegrationTestsBundle/",
            "Akeneo\\Test\\Integration\\": "vendor/akeneo/pim-community-dev/tests/",
            "Pim\\Upgrade\\": "upgrades/"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php", "tests/AppKernelTest.php"],
        "exclude-from-classmap": [
            "vendor/akeneo/pim-community-dev/app/AppKernel.php",
            "vendor/akeneo/pim-community-dev/app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "require": {
        "akeneo/pim-community-dev": "~2.0.40",
        "phpspec/php-diff": "1.0.2"
    },
    "require-dev": {
        "akeneo/phpspec-skip-example-extension": "2.0.*",
        "akeneo/php-coupling-detector": "0.2.0",
        "behat/behat": "3.4.3",
        "behat/gherkin":"4.5.1",
        "behat/mink":"1.7.1",
        "behat/mink-browserkit-driver": "1.3.3",
        "behat/mink-extension": "2.3.1",
        "behat/mink-selenium2-driver": "1.3.x-dev",
        "behat/symfony2-extension": "2.1.4",
        "kriswallsmith/buzz": "^0.15",
        "pdepend/pdepend": "2.1.*",
        "phpmd/phpmd": "1.*",
        "phpspec/phpspec": "3.4.*",
        "phpunit/phpunit": "5.7.*",
        "sebastian/exporter": "2.0.*",
        "liuggio/fastest": "1.6.*",
        "sensiolabs/behat-page-object-extension": "2.1.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
        "akeneo/catalogs": "In order to install one of the Akeneo catalogs"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "php bin/console --ansi fos:js-routing:dump --target=web/js/routes.js",
            "php bin/console --ansi pim:installer:assets"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true,
            "env-map": {
                "database_host": "PIM_DATABASE_HOST",
                "database_port": "PIM_DATABASE_PORT",
                "database_name": "PIM_DATABASE_NAME",
                "database_user": "PIM_DATABASE_USER",
                "database_password": "PIM_DATABASE_PASSWORD",
                "index_hosts": "PIM_INDEX_HOSTS"
            }
        },
        "branch-alias": {
            "dev-master": "2.1.x-dev",
            "dev-2.0": "2.0.x-dev",
            "dev-1.7": "1.7.x-dev",
            "dev-1.6": "1.6.x-dev",
            "dev-1.5": "1.5.x-dev",
            "dev-1.4": "1.4.x-dev",
            "dev-1.3": "1.3.x-dev",
            "dev-1.0": "1.0.x-dev"
        }
    }
}
