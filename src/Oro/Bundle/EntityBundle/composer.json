{
    "name": "oro/entity-bundle",
    "type": "symfony-bundle",
    "description": "BAP Entity Bundle",
    "keywords": ["BAP"],
    "homepage": "https://github.com/laboro/EntityBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "oro/entity-config-bundle": "dev-master",
        "oro/ui-bundle": "dev-master",
        "oro/grid-bundle": "dev-master",
        "oro/navigation-bundle": "dev-master",
        "oro/form-bundle": "dev-master",
        "oro/user-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\EntityBundle": "" }
    },
    "target-dir": "Oro/Bundle/EntityBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
