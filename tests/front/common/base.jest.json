{
  "testPathIgnorePatterns": [
    "node_modules"
  ],
  "moduleDirectories": [
    "node_modules",
    "public/bundles"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json",
    "node"
  ],
  "collectCoverage": true,
  "coverageReporters": [
    "html"
  ],
  "cacheDirectory": "/tmp/jest",
  "testEnvironment": "jsdom",
  "testEnvironmentOptions": {
    "url": "http://localhost/"
  },
  "coveragePathIgnorePatterns": ["./lib/"]
}
